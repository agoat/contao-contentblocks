{
    "name": "agoat/contentblocks-bundle",
    "description": "Contao 3 content blocks extension",
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Arne Stappen (alias aGoat)",
            "homepage": "https://agoat.xyz"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^3.3",
        "contao/core-bundle": "^4.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "replace": {
        "contao-legacy/contentblocks": "*",
        "agoat/contao-contentblocks": "*"
    },
    "autoload": {
        "psr-4": {
            "Agoat\\CustomContentElementsBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    }
}
